pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'Hello build'
                sleep 5
            }
        }
        
        stage('deploy') {
            steps {
                echo 'Hello deploy'
                sleep 4
            }
        }
        
        stage('test') {
            steps {
                echo 'Hello test'
                sleep 3
            }
        }
        
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 2
            }
        }
    }
}
